#include  <stdio.h>


        int main(){ 
        
        int i;
        int numero;
        int acm = 0;
        
		for (i = 0; i < 6; i++) {
		printf("\nDigite um número: ");
		scanf("%d", &numero);

		if (numero > 0) {
			acm++;
			}
		
		}
     printf("A quantidade de positivos é %d ", acm);

return 0;

}
